class Solution {
    public int maxDepth(String s) {
        int res=0;
        int curr=0;
        int n=s.length();
        for(int i=0;i<n;i++){
            if(s.charAt(i)=='('){
                curr++;
                res=Math.max(curr,res);
            }
            else if(s.charAt(i)==')'){
                curr--;
            }
        }
        return res;
    }
}

TC:O(n)
SC:O(1)
